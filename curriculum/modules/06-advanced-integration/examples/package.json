{
  "name": "ens-advanced-integration-examples",
  "version": "1.0.0",
  "description": "Production-ready ENS integration examples with monitoring, security, and optimization",
  "main": "production-ens-manager.js",
  "scripts": {
    "start": "node production-ens-manager.js",
    "dev": "NODE_ENV=development node production-ens-manager.js",
    "prod": "NODE_ENV=production node production-ens-manager.js",
    "test": "jest",
    "lint": "eslint *.js"
  },
  "keywords": [
    "ens",
    "ethereum",
    "production",
    "monitoring",
    "security",
    "optimization"
  ],
  "author": "ENS Developer Education Series",
  "license": "MIT",
  "dependencies": {
    "ethers": "^5.7.2",
    "redis": "^4.6.8",
    "winston": "^3.8.2",
    "prom-client": "^14.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "eslint": "^8.45.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ensdomains/developer-education-series"
  },
  "env": {
    "development": {
      "NODE_ENV": "development",
      "DEV_RPC_URL": "https://goerli.infura.io/v3/YOUR_PROJECT_ID",
      "REDIS_URL": "redis://localhost:6379"
    },
    "production": {
      "NODE_ENV": "production",
      "PROD_RPC_URL": "https://mainnet.infura.io/v3/YOUR_PROJECT_ID",
      "REDIS_URL": "redis://your-redis-instance:6379"
    }
  }
}
